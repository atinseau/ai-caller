{
  "name": "@ai-caller/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "bun --watch src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --format esm --minify --splitting",
    "start": "NODE_ENV=production bun dist/index.js",
    "check-types": "tsc --noEmit",
    "db:re-migrate": "rm -rf prisma/migrations && bunx prisma migrate reset --force && bunx prisma migrate dev -n default && bunx prisma generate"
  },
  "exports": {
    "./types": "./src/types.ts",
    "./openapi": "./dist/openapi.d.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "@ai-caller/shared": "workspace:*",
    "@hono/zod-openapi": "1.2.0",
    "@prisma/adapter-pg": "7.1.0",
    "@prisma/client": "7.1.0",
    "@scalar/hono-api-reference": "0.9.30",
    "better-auth": "1.4.7",
    "cron": "4.4.0",
    "dayjs": "1.11.19",
    "dotenv": "17.2.3",
    "dotenv-expand": "12.0.3",
    "hono": "4.11.1",
    "inversify": "7.10.6",
    "openai": "6.13.0",
    "openapi-typescript": "7.10.1",
    "pino": "10.1.0",
    "pino-pretty": "13.1.3",
    "prisma": "7.1.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "typescript": "5.9.3",
    "zod": "4.3.5"
  }
}
